{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "aseprite-multiple-export",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "fs": {
        "all": true,
        "scope": ["**"]
      },
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "Aseprite",
            "cmd": "Aseprite",
            "args": true
          },
          {
            "name": "cmd.exe",
            "cmd": "cmd.exe",
            "args": true
          }
        ]
      }
    },
    "windows": [
      {
        "title": "aseprite-multiple-export",
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "aseprite-multiple-export",
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "resources": ["assets"]
    }
  }
}
